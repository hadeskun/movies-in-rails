<style>
table, td, th {
    border: 1px solid black;
}

table {
    width: 40%;
}

th {
    height: 25px;
}
td {
    padding: 15px;
}
</style>
<h1>Listing users</h1>

<table>
  <tr>
    <th>Email</th>
  </tr>

<% @users.each do |user| %>
  <tr>
    <td><%= user.email %></td>
    <td><%= link_to 'Show', user %></td>
    <td><%= link_to 'Edit', edit_user_path(user) %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New User', new_user_path %>
